#**** COLORS: ****

source ~/.bashrc.d/catppuccin_mocha

#**** INITIALIZATIONS: ****

# start ssh agent
if [ -z "$SSH_AGENT_PID" ]; then
    eval "$(ssh-agent -s)" > /dev/null 2>&1

fi

# add ssh key
if ! ssh-add -l | grep -q "id_rsa"; then
    ssh-add ~/.ssh/id_rsa > /dev/null 2>&1
fi

#**** ENVIRONMENT ****
export EDITOR="nvim"

#**** ALIASES ****

#**** CUSTOM FUNCTIONS: ****

#**** BASH PROMPT ****

source ~/.bashrc.d/prompt
